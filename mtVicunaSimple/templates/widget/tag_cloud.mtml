	</MTIf>
            <li class="level<MTTagRank$>"><a href="javascript:void(0)" onclick="location.href='<$MTTagSearchLink encode_js="1"$>';return false;" rel="tag nofollow"><$MTTagName$></a></li>
	<MTIf name="__last__">
            </ul>
        </dd>
	</MTIf>
</MTTags>
